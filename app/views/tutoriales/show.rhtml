<% content_support do %>
<% if @tutorial.is_public? then %>
<%=mfcontents_basic("MÃ¡s tutoriales de #{@tutorial.main_category.name}", 
	Tutorial,
	[:published, {:conditions => "tutorials_category_id IN (#{(@tutorial.main_category.all_children_ids.<<(@tutorial.main_category.id)).join(',')})", :order => 'created_on desc', :limit => 5}],
    :cache => "/tutoriales/latest_by_cat_#{@tutorial.tutorials_category_id}",
 	:rating => true,
	:faction_favicon => true,
	:skip_id => @tutorial.id,
	:comments_count => true)%>	
<%=render :partial => 'other_categories', :locals => {:category => @tutorial.tutorials_category} %>
<% else %>
<%= render :partial => '/shared/content_publishing_state', :locals => { :object => @tutorial } %>
<% end -%>
<% end -%>

<% content_main do %> 
  <%=mfcontent(@tutorial)%>
<%end -%>